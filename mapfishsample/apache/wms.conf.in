# %GENERATED_WARNING%
<Location /cgi-bin/mapserv>
   Deny from all
</Location>

Alias /%PROJECT%/%VERSION%/wms /usr/lib/cgi-bin/mapserv
<Location /%PROJECT%/%VERSION%/wms>
   # If you use tilecache and want to prevent direct WMS access:
   Order allow,deny
   Allow from 127.0.0.1

   SetHandler cgi-script
   SetEnv MS_MAPFILE %PROJECT_DIR%/wms/%PROJECT%.map
   SetEnv MS_MAPFILE_PATTERN "^%PROJECT_DIR%/wms/%PROJECT%.map$"
</Location>

